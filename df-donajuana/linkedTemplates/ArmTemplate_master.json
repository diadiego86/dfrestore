{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-donajuana"
		},
		"AmazonS31DonaJuana_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'AmazonS31DonaJuana'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"SADonaJuana_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SADonaJuana'"
		},
		"AzureTableStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureTableStorage1'"
		},
		"AmazonS31DonaJuana_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAZPQ5GGGD3EITUIPW"
		},
		"Trigger_evj_properties_pipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"Trigger_evj_properties_pipeline_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-donajuana_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AmazonS31DonaJuana_secretAccessKey": {
						"value": "[parameters('AmazonS31DonaJuana_secretAccessKey')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"SADonaJuana_connectionString": {
						"value": "[parameters('SADonaJuana_connectionString')]"
					},
					"AzureTableStorage1_connectionString": {
						"value": "[parameters('AzureTableStorage1_connectionString')]"
					},
					"AmazonS31DonaJuana_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS31DonaJuana_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "df-donajuana_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Trigger_evj_properties_pipeline_parameters_windowStart": {
						"value": "[parameters('Trigger_evj_properties_pipeline_parameters_windowStart')]"
					},
					"Trigger_evj_properties_pipeline_parameters_windowEnd": {
						"value": "[parameters('Trigger_evj_properties_pipeline_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-donajuana_ArmTemplate_0')]"
			]
		}
	]
}